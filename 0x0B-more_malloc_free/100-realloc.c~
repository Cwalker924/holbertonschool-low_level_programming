#include "holberton.h"
#include <stdio.h>
#include <stdlib.h>

/**
 *
 *
 *
 *
 */
void *_realloc(void *ptr, unsigned int old_size, unsigned int new_size)
{
	int *c;

	if (ptr == NULL)
	{
		c = malloc(new_size * sizeof(*ptr));
	}

	if (new_size > old_size)
	{
		;
	}
	else if (new_size == old_size)
	{
		return (ptr);
	}
}
